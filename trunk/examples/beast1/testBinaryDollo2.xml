<beast version='2.0'
    namespace='beast.evolution.alignment:beast.core:beast.evolution.tree.coalescent:beast.core.util:beast.evolution.nuc:beast.evolution.operators:beast.evolution.sitemodel:beast.evolution.substitutionmodel:beast.evolution.likelihood'>

    <data id="alignment" spec='AscertainedAlignment'>
		<userDataType spec='beast.evolution.datatype.MutationDeathType' deathChar='0' extantCode='1'/>
        <sequence taxon="Ar"> 
        </sequence>
        <sequence taxon="Dr">            
        </sequence>
        <sequence taxon="An"> 
        </sequence>
        <sequence taxon="Ho">

        </sequence>
        <sequence taxon="Ca"> 
        </sequence>
<!--
        <sequence taxon="Ar"> 000001
        </sequence>
        <sequence taxon="Dr"> 000001
        </sequence>
        <sequence taxon="An"> 110001
        </sequence>
        <sequence taxon="Ho">001111
        </sequence>
        <sequence taxon="Ca"> 000001
        </sequence>
-->
    </data>

    <run spec="MCMC" id="mcmc" chainLength="1000000">

	<state>
		<parameter name='stateNode' id='deathRate' lower="0.0" value="1.0"/>

<!--		<stateNode id='tree' spec='beast.util.TreeParser' newick='(Ar:1.2587335224961815,((Dr:0.48328238018013014,An:0.48328238018013014):0.7573037533374662,(Ho:1.0658133023983523,Ca:1.0658133023983523):0.17477283111924424):0.018147388978585033)'>
(Ar:0.8884959699821183,((Dr:0.13514331794790563,An:0.13514331794790563):0.621722055422071,(Ho:0.7495724084782432,Ca:0.7495724084782432):0.007292964891733478):0.1316305966121416);'>-->
		<stateNode id='tree' spec='beast.util.TreeParser' newick='(Ar:3,((Dr:1,An:1):1,(Ho:1,Ca:1):1):1);'>
			<taxonset id='taxa' spec='TaxonSet' alignment='@alignment'/>
		</stateNode>
	</state>

		<distribution spec='CompoundDistribution' id='posterior'>
			<distribution spec='CompoundDistribution' id="prior">
<!--
				<distribution id='deathRatePrior' spec='Prior' x='@deathRate'>
					<distr spec='Gamma'scale="1" shape="0.0001" offset="0"/>
				</distribution>
-->
			</distribution>

			<distribution spec='CompoundDistribution' id="likelihood">
				<distribution spec='beast.evolution.MSSD.ALSTreeLikelihood' id="treelikelihood" tree='@tree' data='@alignment' useAmbiguities="true">
					<observationprocess spec='beast.evolution.MSSD.AnyTipObservationProcess' tree='@tree' data='@alignment' siteModel='@siteModel' integrateGainRate="true"
						mu='@deathRate'/>
					<siteModel spec='SiteModel' id="siteModel" gammaCategoryCount="1">
						<substModel spec="MutationDeathModel" id="SDollo">
							<deathprob idref="deathRate"/>
							<frequencies id='freqs' spec='Frequencies'>
								<frequencies spec='parameter.RealParameter' value='1 0'/>
							</frequencies>
						</substModel>
					</siteModel>
					<!-- defaults to strict clock branchRateModel idref="branchRates"/ -->
				</distribution>
			</distribution>
		</distribution>

		<operator spec='Uniform' weight="1" tree="@tree"/>
        <operator id='treeScaler' spec='ScaleOperator' scaleFactor="0.5" weight="1" tree="@tree" rootOnly='true'/>

        <logger logEvery="1000" fileName="binaryDollo2.log">
	        <model idref='posterior'/>
            <log idref="posterior"/>
            <log idref="prior"/>
            <log idref="likelihood"/>
            <log spec='beast.evolution.tree.TreeHeightLogger' tree='@tree'/>
        </logger>

        <logger logEvery="10000" fileName="binaryDollo2.trees">
            <log idref="tree"/>
        </logger>

        <logger logEvery="1000">
	        <model idref='likelihood'/>
            <log idref="posterior"/>
    	    <ESS spec='ESS' name='log' arg="@posterior"/>
            <log idref="likelihood"/>
            <log idref='prior'/>
        </logger>
    </run>

</beast>
